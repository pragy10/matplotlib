import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

a=[1,2,3,4,5]
b=[2,4,6,8,10]
c=[3,6,9,12,15]
d=[4,8,12,16,20]

plt.plot(a,b, label='2_tables',color='red',linestyle='solid',linewidth=3,marker='>',markeredgecolor='maroon',markersize=10)
plt.plot(a,c, label='3_tables',color='cyan',linestyle='dashdot',linewidth=3,marker='*',markeredgecolor='darkblue',markersize=15)
plt.xlabel('multiplier')
plt.ylabel('number')
plt.title('table')
plt.legend(loc=2)
plt.grid(True)
plt.show()
